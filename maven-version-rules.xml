<ruleset comparisonMethod="maven"
         xmlns="http://mojo.codehaus.org/versions-maven-plugin/rule/2.0.0"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://mojo.codehaus.org/versions-maven-plugin/rule/2.0.0 https://www.mojohaus.org/versions-maven-plugin/xsd/rule-2.0.0.xsd">
    <ignoreVersions>
        <ignoreVersion type="regex">.*[bB]eta[0-9]*$</ignoreVersion>
        <ignoreVersion type="regex">.*[aA]lpha[0-9]*$</ignoreVersion>
        <ignoreVersion type="regex">.*[rRcC][0-9]*$</ignoreVersion>
        <ignoreVersion type="regex">.*[mM][0-9]*$</ignoreVersion>
        <ignoreVersion type="regex">.*-rc-\d+$</ignoreVersion>
        <ignoreVersion type="regex">[0-9]{8}(\.[0-9]{6})?$</ignoreVersion>
    </ignoreVersions>
</ruleset>